{
  "name": "ng-launchpad",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "ng lint && npm run lint-css",
    "lint:fix": "ng lint --fix && npm run lint-css:fix",
    "lint-css": "stylelint \"src/**/*.scss\" \"src/**/*.html\"",
    "lint-css:fix": "stylelint \"src/**/*.scss\" \"src/**/*.html\" --fix",
    "fmt": "prettier --write \"src/**/*.ts\"",
    "e2e": "node e2e/puppeteer.conf.js",
    "deploy": "ng build --prod --base-href 'https://heyayush.github.io/ng-launchpad/' && npx angular-cli-ghpages --no-silent",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.1.2",
    "@angular/common": "~8.1.2",
    "@angular/compiler": "~8.1.2",
    "@angular/core": "~8.1.2",
    "@angular/forms": "~8.1.2",
    "@angular/platform-browser": "~8.1.2",
    "@angular/platform-browser-dynamic": "~8.1.2",
    "@angular/pwa": "^0.801.2",
    "@angular/router": "~8.1.2",
    "@angular/service-worker": "~8.1.2",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.2",
    "@angular/cli": "~8.1.2",
    "@angular/compiler-cli": "~8.1.2",
    "@angular/language-service": "~8.1.2",
    "@babel/core": "^7.5.5",
    "@storybook/addon-actions": "^5.1.9",
    "@storybook/addon-links": "^5.1.9",
    "@storybook/addon-notes": "^5.1.9",
    "@storybook/addons": "^5.1.9",
    "@storybook/angular": "^5.1.9",
    "@types/jest": "^24.0.15",
    "@types/node": "~8.9.4",
    "@types/puppeteer": "^1.19.0",
    "angular-cli-ghpages": "^0.5.3",
    "babel-loader": "^8.0.6",
    "codelyzer": "^5.0.0",
    "jest": "^24.8.0",
    "jest-preset-angular": "^7.1.1",
    "prettier": "1.18.2",
    "puppeteer": "^1.19.0",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended-scss": "^3.3.0",
    "stylelint-scss": "^3.9.4",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setupJest.ts"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/dist/",
      "<rootDir>/src/test.ts"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/tsconfig.spec.json",
        "diagnostics": false,
        "stringifyContentPathRegex": "\\.html$",
        "astTransformers": [
          "<rootDir>/node_modules/jest-preset-angular/InlineHtmlStripStylesTransformer"
        ]
      }
    }
  }
}
